{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8.0.0"
        },
        "modules": false
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-proposal-object-rest-spread"
      ]
    },
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}
